{
  "version": 1,
  "cloudant_url": {
    "searchPatterns": [
      "cloudfoundry:$['cloudant'][0].credentials.url",
      "env:service_cloudant:$.url",
      "env:cloudant_url",
      "env:CLOUDANT_URL",
      "file:/server/localdev-config.json:$.cloudant_url"
    ]
  },
  "cloudant_services": {
    "searchPatterns": [
      "cloudfoundry:$['cloudantNoSQLDB']",
      "env:cloudant_services",
      "file:/server/localdev-config.json:$.cloudant_services"
    ]
  },
  "node_red_storage": {
    "searchPatterns": [
      "cloudfoundry:NODE_RED_STORAGE_NAME",
      "env:node_red_storage_name",
      "env:NODE_RED_STORAGE_NAME"
    ]
  },
  "node_red_storage_db": {
    "searchPatterns": [
      "cloudfoundry:NODE_RED_STORAGE_DB_NAME",
      "env:node_red_storage_db_name",
      "env:NODE_RED_STORAGE_DB_NAME"
    ]
  },
  "node_red_storage_app": {
    "searchPatterns": [
      "cloudfoundry:NODE_RED_STORAGE_APP_NAME",
      "env:node_red_storage_app_name",
      "env:NODE_RED_STORAGE_APP_NAME"
    ]
  },
  "node_red_user": {
    "searchPatterns": [
      "cloudfoundry:cloudant",
      "env:cloudant_credentials",
      "env:node_red_username",
      "env:NODE_RED_USERNAME",
      "file:/server/localdev-config.json:cloudant_credentials"
    ]
  },
  "node_red_password": {
    "searchPatterns": [
      "cloudfoundry:NODE_RED_PASSWORD",
      "env:node_red_password",
      "env:NODE_RED_PASSWORD"
    ]
  },
  "node_red_guest": {
    "searchPatterns": [
      "cloudfoundry:NODE_RED_GUEST_ACCESS",
      "env:node_red_guest_access",
      "env:NODE_RED_GUEST_ACCESS"
    ]
  },
  "node_red_appmetrics": {
    "searchPatterns": [
      "cloudfoundry:NODE_RED_USE_APPMETRICS",
      "env:node_red_use_appmetrics",
      "env:NODE_RED_USE_APPMETRICS"
    ]
  },
  "cloudantNoSQLDB": {
    "searchPatterns": [
      "cloudfoundry:cloudantNoSQLDB",
      "env:cloudantNoSQLDB"
    ]
  }
}
